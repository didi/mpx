  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/didi/mpx.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@mpxjs/utils": "^2.10.4"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
