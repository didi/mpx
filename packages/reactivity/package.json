{
  "name": "@mpxjs/reactivity",
  "version": "2.10.4",
  "description": "Reactivity core system for Mpx",
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "tsc && tsup",
    "bench:output": "vitest bench __benchmarks__/computed.bench.ts --outputJson=temp/bench.json",
    "bench:compare": "vitest bench __benchmarks__/computed.bench.ts --compare=temp/bench.json",
    "bench": "npm run build && vitest bench",
    "test": "vitest run"
  },
  "keywords": [
    "mpx",
    "reactivity"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/didi/mpx.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@mpxjs/utils": "^2.10.4"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
