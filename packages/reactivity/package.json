{
  "name": "@mpxjs/reactivity",
  "version": "2.10.4",
  "description": "Reactivity system for Mpx",
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "./@types/index.d.ts",
  "exports": {
    ".": {
      "types": "./@types/index.d.ts",
      "require": "./dist/index.cjs.js",
