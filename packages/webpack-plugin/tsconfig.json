{
  "include": [
    "./lib/runtime/components/react"
  ],
  "exclude": [
    "./lib/runtime/components/react/__tests__/**/*",
    "./lib/runtime/components/react/dist"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "outDir": "./lib/runtime/components/react/dist",
    "noImplicitThis": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ]
  }
}
