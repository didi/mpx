      wx:for="{{r.children}}"
      wx:if="{{item.nodeType !== 'h-comment'}}"
      wx:key="nodeId"
      class="{{item.class}}"
      id="{{item.id}}"
      data-private-node-id="{{item.nodeId}}"
    ><template is="children" data="{{r: item.children}}" /></swiper-item></swiper></template>

