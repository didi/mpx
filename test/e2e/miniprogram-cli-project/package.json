{
  "name": "miniprogram-cli-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "mpx-cli-service serve",
    "build": "mpx-cli-service build",
    "lint": "eslint --ext .js,.ts,.mpx src/",
    "build:android": "mpx-cli-service build --targets=android && cd ReactNativeProject && npm run bundle:android",
    "build:ios": "mpx-cli-service build --targets=ios && cd ReactNativeProject && npm run bundle:ios",
    "build:harmony": "mpx-cli-service build --targets=harmony && cd ReactNativeProject && npm run bundle:android",
    "build:ssr": "mpx-cli-service build:ssr",
    "build:web": "mpx-cli-service build --targets=web",
    "serve:android": "cd ReactNativeProject && npm run android && cd .. && mpx-cli-service serve --targets=android",
    "serve:ios": "cd ReactNativeProject && npm run ios && cd .. && mpx-cli-service serve --targets=ios",
    "serve:harmony": "cd ReactNativeProject && npm run android && cd .. && mpx-cli-service serve --targets=harmony",
    "serve:ssr": "mpx-cli-service serve:ssr",
    "serve:web": "mpx-cli-service serve --targets=web",
    "copyPlugin": "sh ./copyWebpackPlugin.sh"
  },
  "dependencies": {
    "@mpxjs/api-proxy": "^2.10.0",
    "@mpxjs/core": "^2.10.0",
    "@mpxjs/fetch": "^2.10.0",
    "@mpxjs/pinia": "^2.10.0",
    "@mpxjs/store": "^2.10.0",
    "@mpxjs/utils": "^2.10.0",
    "pinia": "^2.0.14",
    "react": "18.3.1",
    "react-native": "0.77.2",
    "react-native-reanimated": "3.16.7",
    "vue": "^2.7.0",
    "vue-demi": "^0.14.6",
    "vue-i18n": "^8.27.2",
    "vue-i18n-bridge": "^9.2.2",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/runtime-corejs3": "^7.10.4",
    "@mpxjs/babel-plugin-inject-page-events": "^2.9.0",
    "@mpxjs/eslint-config": "^2.0.3",
    "@mpxjs/mpx-cli-service": "^2.0.0",
    "@mpxjs/size-report": "^2.10.0",
    "@mpxjs/vue-cli-plugin-mpx": "^2.0.0",
    "@mpxjs/vue-cli-plugin-mpx-eslint": "^2.0.0",
    "@mpxjs/vue-cli-plugin-mpx-ssr": "^2.0.0",
    "@mpxjs/webpack-plugin": "^2.10.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.2.4",
    "axios": "^1.6.0",
    "eslint": "^9.0.0",
    "express": "^4.18.2",
    "postcss": "^8.2.6",
    "process": "^0.11.10",
    "serve-favicon": "^2.5.0",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.1.0",
    "vue-server-renderer": "^2.7.14",
    "webpack": "^5.43.0"
  },
  "browserslist": [
    "ios >= 8",
    "chrome >= 47"
  ]
}
